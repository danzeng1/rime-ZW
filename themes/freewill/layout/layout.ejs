<!DOCTYPE HTML>
<html>
<%- partial('_partial/head') %>
<body>
  <%- partial('_partial/navigation') %>
  <% if (is_home()) { %>
    <%- partial('_partial/front_matter') %>
    <div class="home">
      <%- body %>
    </div>
  <% } %>
  <div class="container">
    <% if (!is_home()) { %>
    <div class="content <%- page.content_class %>">
      <%- body %>
    </div>
    <% } %>
    <%- partial('_partial/footer') %> </footer>
  </div> <!-- container-narrow -->
  <%- partial('_partial/after_footer') %>
</body>
</html>
